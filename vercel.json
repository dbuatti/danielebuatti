{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/quote/:slug",
      "destination": "/quotes/:slug"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}